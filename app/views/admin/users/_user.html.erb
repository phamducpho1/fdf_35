<tr>
  <td><%= user.id %></td>
  <td><%= user.name %></td>
  <td><%= user.created_at %></td>
  <td><%= user.updated_at %></td>
  <td>
  <% if current_user.admin? && !current_user?(user) %>
      <%= link_to admin_user_path(user), method: :delete,
        class: "fa fa-trash-o btn btn-danger", data: {confirm: (t "users.index.sure") } do %>
      <% end %>
  <% end %>
  <%= link_to edit_admin_user_path(user), class: "fa fa-pencil-square-o btn btn-danger" do %>
  <% end %>
  </td>
</tr>



